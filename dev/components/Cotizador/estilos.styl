/*COTIZADOR*/

.Cotizador
	&-titleContainer
		display inline-block
		margin-top 2.5em
		vertical-align top
	&-title
		font-weight 400
		font-size 2.56em
		color $full_black
		margin 0
	&-borde
		width 245px
		height 3px
		background-color $light_gray
		display inline-block
/*Desktop*/
@media (min-width: 1100px)
	.Cotizador
		margin-top 120px